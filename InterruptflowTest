import unittest
from Interuptflow import Task, Member, ActivityType

class TestTask(unittest.TestCase):

    def setUp(self):
        self.task = Task(1, 1, 1, 1, 1,1,1,1,1)  # Erstellen Sie eine Aufgabe mit einer ID von 1, einer Priorit√§t von 1, einer Ramp-Up-Zeit von 1, einer Ramp-Down-Zeit von 1 und einer Arbeitszeit von 5
        self.member = Member(1, "Test Member")

    def test_work(self):
        self.task.start_working()
        self.task.work([self.member],1)
        self.assertEqual(len(self.task.activity_log), 1)
        

if __name__ == '__main__':
    unittest.main()
